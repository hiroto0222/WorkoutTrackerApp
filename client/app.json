{
  "expo": {
    "name": "Gym Mate",
    "slug": "gym-mate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "image": "./src/assets/splash-screen.png"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hiroto0222.workouttracker"
    },
    "android": {
      "package": "com.hiroto0222.workouttracker"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "137520aa-7615-4172-8be1-088338fdd756"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "scheme": "workout-tracker"
  }
}
